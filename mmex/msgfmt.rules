<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="msgfmt"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="msgfmt"
			DisplayName="msgfmt"
			CommandLine="msgfmt.exe [inputs] -o runtime\en\$(InputName).mo"
			Outputs="runtime\en\$(InputName).mo"
			FileExtensions="*.po"
			ExecutionDescription="msgfmt [inputs] -o runtime\en\$(InputName).mo"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
