<SCRIPT>
function convertDate(str, format) {
	var	date= new Date(str.replace("-", "/")); 
	var f = {Y : date.getFullYear(), y : date.getYear(), m : date.getMonth() + 1, d : date.getDate()}
	for(var k in f)
	    format = format.replace(('%' + k), ('0'+f[k]).slice(k=='Y' ? -4 : -2));	
	return format;
};
function printDate(format) {
            var currentdate = new Date(); 
            var datetime = ('0'+currentdate.getDate()).slice(-2) + "/"
                + ('0'+(currentdate.getMonth()+1)).slice(-2)  + "/" 
                + currentdate.getFullYear() + " @ "  
                +  ('0'+currentdate.getHours()).slice(-2) + ":"  
                +  ('0'+currentdate.getMinutes()).slice(-2) + ":" 
                +  ('0'+currentdate.getSeconds()).slice(-2)
            return datetime;
        }
</SCRIPT>

